   "source": [
    "from IPython.display import display, Markdown\n",
    "\n",
    "# 读取Markdown文件内容\n",
    "with open('git-learning-notebook.md', 'r', encoding='utf-8') as file:\n",
    "    markdown_text = file.read()\n",
    "\n",
    "# 在Notebook中渲染Markdown文本\n",
    "display(Markdown(markdown_text))"
